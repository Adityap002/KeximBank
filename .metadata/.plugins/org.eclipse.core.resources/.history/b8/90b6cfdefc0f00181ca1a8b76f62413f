package com.KeximBank.master;

import org.openqa.selenium.WebElement;
import org.openqa.selenium.support.FindBy;

public class EmpHP {
	@FindBy (xpath = "//a[@href='adminflow.aspx']") WebElement empHomeBtn;
	@FindBy (id = "BtnNew") WebElement newEmp;
	
	public void empHomeButton(){
		empHomeBtn.click();
	}
	
	public void newEmpButton(){
		newEmp.click();
	}

}
